<?php

function calacademy_science_today_app_version_feed_preprocess_html(&$vars) {
	if (request_path() != 'module/science_today_app_version/json') return;

	http_response_code(200);
	header('Content-type: application/json');

	ob_start();
	include 'science-today-app-version-feed.json.php';
	die(ob_get_clean());
}
