			#explore-library {
				max-width: 100%;
				margin-top: $padding * 4;

				.creature-description {
					width: 30%;
					left: -140px;

					h2 {
						max-width: 300px;
					}

					p {
						max-width: 450px;
					}

					@include respond-to(tablet) {
						h2, p {
							width: 150%;
						}
					}
				}

				img {
					margin-top: $padding * 2;
					width: 70%;
					max-width: 1200px;
					left: -100px;
				}

				@include respond-to(smartphone) {
					.creature-description,
					img {
						width: 100%;
						left: 0;
						top: 0;
						margin: 0;
						float: none;
					}

					.creature-description {
						padding-left: $smartphonePadding;
						padding-right: $smartphonePadding;

						h2 {
							width: 50%;
						}

						p {
							width: 100%;
						}
					}

					img {
						margin-top: $padding;
						margin-left: $padding * 3;
					}
				}
			}

			#people {
				margin-top: $squareWidth - 40px;

				.views-field-field-subtitle {
					display: none;
				}

				.pane-title,
				& > p {
					width: 410px;

					@include respond-to(smartphone) {
						width: inherit;
						margin-bottom: $padding;
					}
				}

				.pane-title {
					font-size: $headerTextReduced;
					position: absolute;
					top: -100px;

					@include respond-to(smartphone) {
						width: inherit;
						position: relative;
						top: inherit;
					}
				}

				.views-row-4 {
					margin-bottom: $padding * 2;
				}
			}

			#footer {
				max-width: 100%;
				margin-bottom: 0;
				padding-bottom: 0;	
			}

			@include respond-to(smartphone) {
				#events,
				#animal-ambassadors,
				#explore-science,
				#explore-library,
				#people {
					margin-top: inherit;
					margin-bottom: $padding * 2;

					h2,
					.pane-title {
						font-size: $largeText;
					}	
				}

				#events,
				#people {
					padding-left: $smartphonePadding;
					padding-right: $smartphonePadding;
				}

				nav {
					margin-bottom: $padding * 2;
				}
			}

			@include respond-to(tablet) {
				#events,
				#people {
					margin-left: $padding;
				}
			}				
		}
	}
}
