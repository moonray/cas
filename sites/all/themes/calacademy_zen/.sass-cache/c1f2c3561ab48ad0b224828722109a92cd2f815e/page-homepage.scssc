:@template"¿ @import "global-variables";

.page-homepage {
	#main {
		border-bottom: 30px solid $dark-grey;
		color: $text-grey;

		h1, h2, h3, h4 {
			line-height: 1em;
		}

		#content {
			max-width: none;

			@include respond-to(smartphone) {
				padding-left: 0;
				padding-right: 0;
			}

			@include respond-to(tablet) {
				padding-left: 0;
				padding-right: 0;
			}

			.panel-pane {
				position: relative;
				@include box-sizing(border-box);
				max-width: $maxWidth;
				margin-left: auto;
				margin-right: auto;

				& > p {
					font-size: $slightlyLargerText;
					line-height: $slightlyLargerTextLineHeight;

					@include respond-to(smartphone) {
						font-size: $smartphoneText;
						line-height: $smartphoneLineHeight;
					}
				}
			}

			// only used as source material for JS
			#people-description,
			.pane-testimonials-related {
				display: none;
			}

			.testimonial {
				width: $squareWidth;
				
				@include respond-to(tablet) {
					width: $squareWidthTablet;
				}

				@include respond-to(smartphone) {
					@include box-sizing(border-box);
					position: relative;
					top: inherit;
					float: none;
					width: 100%;
					padding-top: $padding;
				}
			}

			.creature {
				width: 100%;
				clear: both;
				overflow: hidden;

				.creature-description,
				img {
					position: relative;
					float: right;
					width: 50%;
					margin: 0;
					padding: 0;
					left: 0;
					top: 0;
				}

				.creature-description {
					@include box-sizing(border-box);
					word-wrap: normal;
					z-index: 2;

					h2, p {
						width: 100%;
					}

					a {
						@include arrow(arrow-orange);
						color: $international-orange;
					}
				}

				img {
					z-index: 1;
				}
			}

			.proto-slideshow {
				max-width: none;
				width: 100%;
				background-color: $black;
			}

			/*
			
				id level specifics

			*/

			#hero {
				max-width: inherit;
				line-height: 0;

				img {
					width: 100%;

					&.tablet-desktop {
						display: block;
					}

					&.smartphone {
						display: none;
					}
				}

				@include respond-to(smartphone) {
					img {
						&.tablet-desktop {
							display: none;
						}

						&.smartphone {
							display: block;
						}
					}
				}
			}

			#animal-ambassadors {
				margin-top: ($padding * 2) + 75;
				margin-bottom: $padding * 2;
				left: -120px;

				.creature-description {
					padding-right: $padding;
					overflow: visible;
					width: 35%;

					top: 100px;
					left: -120px;

					h2, p {
						width: 120%;

						@include respond-to(tablet) {
							width: 150%;
						}
					}
				}

				img {
					width: 65%;
				}

				// sea urchin gif prototype
				&.gif-test {
					img {
						left: -40px;
					}

					padding-bottom: 100px;

					@include respond-to(smartphone) {
						padding-bottom: 0;
					}	
				}

				@include respond-to(smartphone) {
					left: 0;

					.creature-description {
						width: 30%;
						padding-right: $smartphonePadding;

						top: 0;
						left: -20%;

						h2, p {
							width: 170%;
						}
					}

					img {
						left: -50px;
						width: 70%;
					}
				}

				@include respond-to(smartphone-landscape) {
					.creature-description {
						left: -90px;
					}	
				}
			}

			#events {
				z-index: 3;
				overflow: hidden;
				margin-top: -250px;

				.pane-title {
					position: absolute;
					width: 340px;
					top: $squareWidth + ($padding * 2);
					font-size: $headerTextReduced;

					@include respond-to(smartphone) {
						position: relative;
						top: inherit;
						width: inherit;
					}
				}

				@include respond-to(tablet) {
					margin-top: -170px;

					.pane-title {
						top: $squareWidthTablet;	
					}	
				}

				.view {
					left: 50%;

					@include respond-to(smartphone) {
						left: 0;

						// .views-field-field-date {
						// 	display: none;
						// }
					}
				}
			}

			#explore-science {
				max-width: inherit;
				background-color: #030400;
				margin-top: $padding * 3;	
			}

			/*
			#explore-science {
				$maxHeight: 630px;
				
				max-width: inherit;
				background-color: #030400;
				margin-top: $padding * 3; 

